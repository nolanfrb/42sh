[2601]
NAME="REDIRECTION_HEREDOC: Here-document redirection <<"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo "cat << EOF
Hello World
This is a multiline test
EOF"
[2601-END]

[2602]
NAME="REDIRECTION_HEREDOC: Here-document with special delimiter"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo "cat << CUSTOM_DELIM
Custom delimiter test
Multiple lines
CUSTOM_DELIM"
[2602-END]