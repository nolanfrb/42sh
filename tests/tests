[3302]
NAME="SUBSHELL: Subshell with pipes"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo "(ls -la | grep d)"
[3302-END]

[3303]
NAME="SUBSHELL: Subshell with semicolons"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo "(cd /tmp ; pwd)"
[3303-END]

[3304]
NAME="SUBSHELL: Nested subshells"
